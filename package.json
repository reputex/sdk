{
  "name": "reputex-sdk-test",
  "version": "1.0.0",
  "description": "Reputex SDK for JavaScript and TypeScript",
  "keywords": [
    "ethereum",
    "binance",
    "avalanche",
    "polygon",
    "blockchain",
    "crypto",
    "eth"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nordfinance/reputex-sdk.git"
  },
  "author": "Nord Finance",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nordfinance/reputex-sdk/issues"
  },
  "homepage": "https://github.com/nordfinance/reputex-sdk#readme",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^0.24.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
